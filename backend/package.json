{
  "name": "batufo-backend",
  "version": "0.0.0",
  "description": "TypeScript packages.",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "(cd ./packages/core && yarn tsc) && (cd ./packages/server && yarn tsc) && (cd ./packages/level-editor && yarn tsc)",
    "start": "cd ./packages/server && DEBUG_COLORS=1 DEBUG='(app|stats|game*):(debug|info|error)' node ./dist/app.js",
    "start:dev": "cd ./packages/server && DEBUG_COLORS=1 DEBUG='(app|stats|game*):(debug|info|error)' ts-node ./src/app.ts",
    "gc:init": "gcloud config set project batufo-game",
    "gc:deploy": "gcloud app deploy",
    "gc:console": "gcloud app open-console",
    "gc:log": "gcloud app logs read",
    "gc:tail": "gcloud app logs tail"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "prettier": "^2.0.5",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.9.3"
  }
}
