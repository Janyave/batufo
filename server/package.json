{
  "name": "batufo-server",
  "version": "0.0.0",
  "description": "Socket.io server for batufo game.",
  "main": "app.js",
  "scripts": {
    "build": "tsc && cp -R ./src/generated ./dist/generated",
    "watch": "tsc --watch",
    "start": "DEBUG_COLORS=1 DEBUG='app|gam*:debug|info|error' node ./dist/app.js",
    "gc:init": "gcloud config set project batufo-game",
    "gc:deploy": "gcloud app deploy",
    "gc:console": "gcloud app open-console",
    "gc:log": "gcloud app logs read",
    "gc:tail": "gcloud app logs tail"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "debug": "^4.1.1",
    "google-protobuf": "^3.12.0",
    "socket.io": "^2.3.0",
    "supports-color": "^7.1.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/google-protobuf": "^3.7.2",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.9.3"
  }
}
