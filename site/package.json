{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c && cp ./public/build/bundle.js* ../build/",
    "dev": "rollup -c -w",
    "format": "prettier --write --plugin-search-dir=. \"{,!(node_modules,public)/**/}*.{svelte,js}\"",
    "start": "sirv public -s"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "prettier": "^2.0.2",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-preprocess-markdown": "^2.7.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4",
    "svelte-routing": "^1.4.2"
  }
}
